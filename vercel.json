{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/analytics.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/llm.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/analytics(.*)",
      "dest": "api/analytics.py"
    },
    {
      "src": "/llm(.*)",
      "dest": "api/llm.py"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
